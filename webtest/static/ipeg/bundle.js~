    var savingWithUTTOSeries$ = _rx.Observable.combineLatest(siteVisitCost$, cableCutCost$, trainingNewFieldTechCost$, function (s, c, t) {
        var s1 = s - s * .15;
        var c1 = c - c * .20;
        var t1 = t - t * .20;
        var y1 = s - s1 + (c - c1) + (t - t1);
        var sIncrease1 = s + s * .15;
        var s2 = sIncrease1 - sIncrease1 * .30;
        var c2 = c - c * .25;
        var t2 = t - t * .30;
        var y2 = sIncrease1 - s2 + (c - c2) + (t - t2);
        var sIncrease2 = sIncrease1 + sIncrease1 * .15;
        var s3 = sIncrease2 - sIncrease2 * .40;
        var c3 = c - c * .35;
        var t3 = t - t * .35;
        var y3 = sIncrease2 - s3 + (c - c3) + (t - t3);

        return [y1, y2, y3];
    });

    var threeYearProjectedROI$ = _rx.Observable.combineLatest(savingWithUTTOSeries$, uttoAnnualCost$, totalMappingCost.vTree.value$, function (s, u, t) {
        return s.reduce(function (x, y) {
            return x + y;
        }) - u * 3 - t;
    });

    function roiYearFormula(siteVisitCost, cableCutCost, trainingNewFieldTechCost, annualCostOfFieldTech, numberOfFieldTechs, numberOfTicketReceivedAYear, costOfOneIncoming811, siteVisitCostWithUTTO, cableCutCostWithUTTO, trainingNewFieldTechCostWithUTTO, uttoAnnualCost, totalMappingCost) {
        var year = siteVisitCost + cableCutCost + trainingNewFieldTechCost + annualCostOfFieldTech * numberOfFieldTechs + numberOfTicketReceivedAYear * costOfOneIncoming811 - (siteVisitCostWithUTTO + cableCutCostWithUTTO + trainingNewFieldTechCostWithUTTO + uttoAnnualCost + totalMappingCost + annualCostOfFieldTech * numberOfFieldTechs + numberOfTicketReceivedAYear * costOfOneIncoming811);
        return year;
    }

