function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { "default": obj }; }

var _HighCharts = require('HighCharts');

var _HighCharts2 = _interopRequireDefault(_HighCharts);

var charts = {};

_HighCharts2["default"].setOptions({
    lang: {
        symbols: ["k", "M", "Billions", "T", "P", "E"]
    }
});

function makeHighChartDriver() {
    return function (chartSink$) {
        chartSink$.map(function (chartSink) {
            return getChartBySink(chartSink);
        }).subscribe(function (chart) {
            return chart.sideEffect(chart.highChart);
        });
    };
}

function getChartBySink(chartSink) {
    var id = chartSink.renderTo;
    if (!charts[id]) {
        charts[id] = {
            highChart: new _HighCharts2["default"].Chart(chartSink.definition),
            sideEffect: null
        };
    }
    charts[id].sideEffect = chartSink.sideEffect;
    return charts[id];
}

exports["default"] = makeHighChartDriver;
module.exports = exports["default"];

