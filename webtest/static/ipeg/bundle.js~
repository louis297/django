    uttoAnnualCost, //c8
    totalMappingCost) {
        //(J17+J22+J26)+(C8×C16)+(J15×C4)
        var year1 = roiYearFormula(siteVisitCost, cableCutCost, trainingNewFieldTechCost, annualCostOfFieldTech, numberOfFieldTechs, numberOfTicketReceivedAYear, costOfOneIncoming811, siteVisitCostWithUTTO, cableCutCostWithUTTO, trainingNewFieldTechCostWithUTTO, uttoAnnualCost, totalMappingCost);
        return year1;
    });

